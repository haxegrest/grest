package grest.docs.v1.types;
typedef RgbColor = {
	/**
		The blue component of the color, from 0.0 to 1.0.
	**/
	@:optional
	var blue : Float;
	/**
		The green component of the color, from 0.0 to 1.0.
	**/
	@:optional
	var green : Float;
	/**
		The red component of the color, from 0.0 to 1.0.
	**/
	@:optional
	var red : Float;
}